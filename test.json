{
	"imports": {
		"IPost": "@shared/types/entities/post.interface",
		"INewPost": "@shared/types/entities/post.interface",
		"IProfile": "@shared/types/entities/profile.interface",
		"IQuestion": "@shared/types/entities/question.interface"
	},
	"actionGroups": {
		"post": {
			"actions": {
				"sendPost": {
					"alias": "send",
					"payload": {
						"newPost": "INewPost"
					}
				},
				"sentPost": {
					"alias": "sent",
					"payload": {
						"post": "IPost",
						"profile": "IProfile",
						"question?": "IQuestion",
						"receivedAt": "number",
						"tmpId": "string"
					}
				}
			}
		}
	}
}
